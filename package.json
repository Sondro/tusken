{
  "name": "tusken",
  "version": "0.1.0",
  "author": "Alec Larson",
  "license": "See LICENSE.md",
  "keywords": [
    "postgres",
    "client",
    "query",
    "builder",
    "typescript",
    "codegen"
  ],
  "types": "dist/tusken.d.ts",
  "module": "dist/tusken.mjs",
  "main": "dist/tusken.cjs",
  "exports": {
    ".": {
      "types": "./dist/tusken.d.ts",
      "import": "./dist/tusken.mjs",
      "default": "./dist/tusken.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "default": "./dist/config.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "release": "dotenv -- multi-semantic-release"
  },
  "prettier": "@alloc/prettier-config",
  "devDependencies": {
    "@alloc/prettier-config": "^1.0.0",
    "@tusken/cli": "workspace:*",
    "@types/node": "^18.7.9",
    "@types/pg": "^8.6.5",
    "dotenv-cli": "^6.0.0",
    "multi-semantic-release": "^3.0.0",
    "pg": "^8.8.0",
    "pg-mem": "^2.6.3",
    "pg-query-stream": "^4.2.4",
    "postgres-interval": "^4.0.0",
    "postgres-range": "^1.1.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unbuild": "^0.8.9",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "@alloc/types": "^2.2.1"
  },
  "peerDependencies": {
    "pg": ">=8.0.0",
    "postgres-interval": "*",
    "postgres-range": "*"
  }
}
