{
  "name": "tusken",
  "version": "1.0.0-alpha.5",
  "author": "Alec Larson",
  "license": "See LICENSE.md",
  "keywords": [
    "postgres",
    "client",
    "query",
    "builder",
    "typescript",
    "codegen"
  ],
  "types": "dist/tusken.d.ts",
  "module": "dist/tusken.mjs",
  "main": "dist/tusken.cjs",
  "exports": {
    ".": {
      "types": "./dist/tusken.d.ts",
      "import": "./dist/tusken.mjs",
      "default": "./dist/tusken.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "default": "./dist/config.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alloc/tusken"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild"
  },
  "devDependencies": {
    "@types/node": "^18.7.9",
    "postgres-interval": "^4.0.0",
    "postgres-range": "^1.1.3",
    "unbuild": "^0.8.9"
  },
  "dependencies": {
    "@alloc/types": "^2.2.1",
    "@types/pg": "^8.6.5"
  },
  "peerDependencies": {
    "pg": ">=8.0.0",
    "postgres-interval": "*",
    "postgres-range": "*"
  }
}
